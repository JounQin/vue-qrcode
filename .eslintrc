{
  "root": true,
  "extends": "@1stg",
  "overrides": [
    {
      "files": [
        "*.{ts,tsx,vue}"
      ],
      "rules": {
        "vue/multi-word-component-names": "off",
        "vue/require-default-prop": "off"
      }
    },
    {
      "files": [
        "*.{ts,tsx}"
      ],
      "rules": {
        "@typescript-eslint/consistent-type-imports": "error"
      }
    }
  ]
}
